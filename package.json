{
  "name": "personal-dashboard",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "dev": "concurrently -k \"yarn build:client:dev\" \"yarn dev:server\"",
    "prebuild": "rimraf dist",
    "build": "yarn build:client && yarn build:server",
    "build:client:dev": "webpack --mode development  --config webpack.config.client.js --watch",
    "build:client": "webpack --mode production --config webpack.config.client.js",
    "build:server": "nest build",
    "start:server": "nest start",
    "dev:server": "nest start --watch",
    "debug:server": "nest start --debug --watch",
    "start:prodserver": "node dist/server/main"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-react": "7.14.5",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/styled-components": "^5.1.25",
    "@types/supertest": "^2.0.11",
    "assert": "^2.0.0",
    "babel-loader": "8.2.2",
    "bundle-loader": "0.5.6",
    "concurrently": "^7.0.0",
    "esbuild-loader": "^2.19.0",
    "html-webpack-plugin": "5.3.2",
    "jest": "^27.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "url": "^0.11.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "axios": "^0.26.1",
    "dotenv": "^16.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "styled-components": "^5.3.1"
  }
}
